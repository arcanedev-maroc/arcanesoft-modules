{
    "name":        "arcanesoft/foundation",
    "description": "ARCANESOFT - CMS built with Laravel Framework.",
    "keywords":    ["arcanedev", "arcanesoft", "cms", "framework", "laravel"],
    "license":     "MIT",
    "type":        "project",
    "authors":     [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "require": {
        "php":                            "^7.2.5",
        "ext-json":                       "*",
        "arcanedev/breadcrumbs":          "^5.0",
        "arcanedev/laravel-active":       "^3.0",
        "arcanedev/laravel-html":         "^7.0",
        "arcanedev/gravatar":             "^4.0",
        "arcanedev/laravel-impersonator": "^3.0",
        "arcanedev/laravel-metrics":      "^2.0",
        "arcanedev/laravel-policies":     "^2.0",
        "arcanedev/log-viewer":           "^7.0",
        "arcanedev/notify":               "^6.0",
        "arcanedev/route-viewer":         "^3.0",
        "arcanedev/support":              "^7.1.1"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit":     "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Arcanesoft\\Foundation\\": "src/"
        },
        "files": ["helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanesoft\\Foundation\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arcanesoft\\Foundation\\FoundationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}

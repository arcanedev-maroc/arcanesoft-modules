{
    "name": "arcanesoft/foundation",
    "description": "ARCANESOFT - CMS built with Laravel Framework.",
    "keywords": ["arcanedev", "arcanesoft", "cms", "framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "require": {
        "php":                            "^7.3",
        "ext-json":                       "*",
        "arcanedev/breadcrumbs":          "^6.0",
        "arcanedev/laravel-active":       "^4.0",
        "arcanedev/laravel-html":         "^8.0",
        "arcanedev/gravatar":             "^5.0",
        "arcanedev/laravel-impersonator": "^4.0",
        "arcanedev/laravel-metrics":      "^3.0",
        "arcanedev/laravel-policies":     "^3.0",
        "arcanedev/log-viewer":           "^8.0",
        "arcanedev/notify":               "^7.0",
        "arcanedev/route-viewer":         "^4.0",
        "arcanedev/support":              "^8.0",
        "arcanesoft/socialite":           "@dev",
        "composer/composer":              "^1.10"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit":     "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Arcanesoft\\Foundation\\": "src/"
        },
        "files": ["helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanesoft\\Foundation\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arcanesoft\\Foundation\\FoundationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}

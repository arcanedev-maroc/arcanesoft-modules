{
    "name":        "arcanesoft/foundation",
    "description": "ARCANESOFT - CMS built with Laravel Framework.",
    "keywords":    ["arcanedev", "arcanesoft", "cms", "framework", "laravel"],
    "license":     "MIT",
    "type":        "project",
    "authors":     [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "require": {
        "php":                        ">=7.2.0",
        "arcanedev/breadcrumbs":      "^4.0",
        "arcanedev/laravel-active":   "^2.0",
        "arcanedev/laravel-html":     "^6.0",
        "arcanedev/laravel-metrics":  "*",
        "arcanedev/laravel-policies": "@dev",
        "arcanedev/log-viewer":       "^5.0",
        "arcanedev/notify":           "^5.0",
        "arcanedev/route-viewer":     "^2.0",
        "arcanesoft/support":         "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "phpunit/phpcov":  "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanesoft\\Foundation\\": "src/"
        },
        "files": ["helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanesoft\\Foundation\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arcanesoft\\Foundation\\FoundationServiceProvider"
            ]
        }
    }
}

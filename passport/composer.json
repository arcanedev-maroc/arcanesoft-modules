{
    "name":        "arcanesoft/passport",
    "description": "ARCANESOFT - Passport module.",
    "keywords":    ["arcanedev", "arcanesoft", "passport", "laravel"],
    "license":     "MIT",
    "type":        "project",
    "authors":     [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "require": {
        "php":                   "^7.3",
        "arcanesoft/foundation": "@dev",
        "laravel/passport":      "^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Arcanesoft\\Passport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanesoft\\Passport\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arcanesoft\\Passport\\PassportServiceProvider"
            ]
        },
        "arcanesoft": {
            "install": "Arcanesoft\\Passport\\Console\\InstallCommand",
            "publish": "Arcanesoft\\Passport\\Console\\PublishCommand"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

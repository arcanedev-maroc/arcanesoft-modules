{
    "name":        "arcanesoft/backups",
    "description": "Backups module for ARCANESOFT.",
    "homepage":    "https://github.com/ARCANESOFT/Backups",
    "keywords":    ["arcanedev", "arcanesoft", "database", "backups"],
    "authors":  [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php": ">=7.2.0",
        "arcanesoft/foundation": "@dev",
        "spatie/laravel-backup": "^6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "phpunit/phpcov":  "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanesoft\\Backups\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanesoft\\Backups\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arcanesoft\\Backups\\BackupsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
